
@font-face {
  font-family: 'Cantarell';
  src: url('<%= asset_path('Enriqueta.ttf') %>');
  font-weight: normal;
  font-style: normal;
}


@font-face {
    font-family: 'LatoHairline';
    src: url('<%= asset_path('Lato-Hai-webfont.eot') %>');
    src: url('<%= asset_path('Lato-Hai-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('Lato-Hai-webfont.woff') %>') format('woff'),
         url('<%= asset_path('Lato-Hai-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('Lato-Hai-webfont.svg#LatoHairline') %>') format('svg');
    font-weight: normal;
    font-style: normal;

}

